public class Solution {
    public int NumJewelsInStones(string J, string S) {
        int count=0;
        if(!(String.IsNullOrEmpty(J) || String.IsNullOrEmpty(S)))
        {
            foreach(var jewel in J)
                foreach(var stone in S)
                    {
                    if(stone.Equals(jewel))
                    {
                        count++;
                    }
                    
                }
            
        }
        return count;
    }
}
