public class Solution {
    public void ReverseString(char[] s) {
     int arraySize=s.Length;
     int i=0;
            for(int j=arraySize-1;j>arraySize/2;j--)
            {            
                char temp=s[i];
                s[i]=s[j];
                s[j]=temp;                
                i++;                
            }     
    }   
}
